p id="notice"><%= notice %></p>

<h1>Listing Freshers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Enroll</th>
      <th>Skills</th>
      <th>Email</th>
      <th>Contact</th>
      <th>Room</th>
      <th>Branch</th>
      <th>Interests</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @freshers.each do |fresher| %>
      <tr>
        <td><%= fresher.name %></td>
        <td><%= fresher.enroll %></td>
        <td><%= fresher.skills %></td>
        <td><%= fresher.email %></td>
        <td><%= fresher.contact %></td>
        <td><%= fresher.room %></td>
        <td><%= fresher.branch %></td>
        <td><%= fresher.interests %></td>
        <td><%= link_to 'Show', fresher %></td>
        <td><%= link_to 'Edit', edit_fresher_path(fresher) %></td>
        <td><%= link_to 'Destroy', fresher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fresher', new_fresher_path %>
