<p id="notice"><%= notice %></p>

<h1>Listing Freshers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Enroll</th>
      <th>Skills</th>
      <th>Email</th>
      <th>Contact</th>
      <th>Room</th>
      <th>Branch</th>
      <th>Interests</th>
      <th>About</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @freshers.each do |fresher| %>
      <tr>
        <td><%= fresher.name %></td>
        <td><%= fresher.enroll %></td>
        <td><%= fresher.skills %></td>
        <td><%= fresher.email %></td>
        <td><%= fresher.contact %></td>
        <td><%= fresher.room %></td>
        <td><%= fresher.branch %></td>
        <td><%= fresher.interests %></td>
        <td><%= fresher.about %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
